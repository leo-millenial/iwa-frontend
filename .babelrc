{
  "presets": [
    "patronum/babel-preset",
    "atomic-router/babel-preset"
  ],
  "plugins": [
    ["effector/babel-plugin",
      {
        "factories": ["patronum", "@withease/factories", "atomic-router"],
        "hmr": "es"
      }
    ]
  ],
  "env": {
    "production": {
      "plugins": [
        ["effector/babel-plugin",
          {
            "factories": ["patronum", "@withease/factories", "atomic-router"],
            "hmr": "none"
          }
        ]
      ]
    }
  }
}
