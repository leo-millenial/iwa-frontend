{
  "presets": [
    "patronum/babel-preset"
  ],
  "plugins": [
    ["effector/babel-plugin",
      {
        "factories": ["patronum", "@withease/factories"],
        "hmr": "es"
      }
    ]
  ],
  "env": {
    "production": {
      "plugins": [
        ["effector/babel-plugin",
          {
            "factories": ["patronum", "@withease/factories"],
            "hmr": "none"
          }
        ]
      ]
    }
  }
}
